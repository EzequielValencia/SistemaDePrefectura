<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="400.0" prefWidth="400.0" styleClass="container" stylesheets="@login.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <Label fx:id="labelMensaje" layoutX="40.0" layoutY="67.0" prefHeight="48.0" prefWidth="321.0" textAlignment="RIGHT" textFill="#23bb58" wrapText="true">
         <font>
            <Font name="SansSerif Bold" size="19.0" />
         </font>
      </Label>
      <JFXTextField fx:id="textUsuario" labelFloat="true" layoutX="42.0" layoutY="152.0" prefHeight="25.0" prefWidth="317.0" promptText="Usuario" styleClass="form-control" unFocusColor="#c61a1a" />
      <JFXPasswordField fx:id="textPassword" labelFloat="true" layoutX="42.0" layoutY="223.0" prefHeight="38.0" prefWidth="317.0" promptText="Password" styleClass="form-control" />
      <JFXButton fx:id="botonIngresar" layoutX="42.0" layoutY="305.0" prefHeight="48.0" prefWidth="120.0" text="Ingresar">
         <font>
            <Font name="SansSerif Regular" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="botonCerrar" layoutX="239.0" layoutY="305.0" prefHeight="48.0" prefWidth="120.0" text="Cerrar" />
   </children>
</Pane>
